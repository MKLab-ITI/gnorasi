@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix rep: <http://www.openrdf.org/config/repository#>.
@prefix sr: <http://www.openrdf.org/config/repository/sail#>.
@prefix sail: <http://www.openrdf.org/config/sail#>.
@prefix owlim: <http://www.ontotext.com/trree/owlim#>.

[] a rep:Repository ;
	rep:repositoryID "gnorasi" ;
	rdfs:label "GNORASI repository" ;
	rep:repositoryImpl [
		rep:repositoryType "openrdf:SailRepository" ;
		sr:sailImpl [
			sail:sailType "swiftowlim:Sail" ;
			owlim:ruleset "owl-horst-optimized" ;
			owlim:noPersist "true" ;
			owlim:storage-folder "storage" ;
			owlim:base-URL "http://www.gnorasi.gr/ontology#" ;
			owlim:new-triples-file "new-triples-file.owl" ;
			owlim:entity-index-size "200000" ;
			owlim:jobsize "200" ;
			owlim:repository-type "in-memory-repository" ;
			owlim:imports "./ontology/gnorasi.owl " ;
			owlim:defaultNS "http://www.gnorasi.gr/ontology#" .
		]
	].